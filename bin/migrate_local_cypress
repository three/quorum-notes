#!/usr/bin/env zsh
source "$QUORUM_TOOLS_DIR/quorum.sh"
dc run --rm server bash -c 'USE_LOCAL_DB=True USE_PROD_DB=False python manage.py flush --no-input'
dc run --rm server bash -c 'USE_LOCAL_DB=True USE_PROD_DB=False python manage.py migrate --run-syncdb --settings=test_settings'
dc run --rm server bash -c 'USE_LOCAL_DB=True USE_PROD_DB=False python manage.py seed_cypress'
